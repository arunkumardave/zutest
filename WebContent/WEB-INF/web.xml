<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>zutest</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
  
  	<servlet>
		<servlet-name>HomeServlet</servlet-name>
		<servlet-class>com.zoho.HomeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>HomeServlet</servlet-name>
		<url-pattern>/home</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
    	<servlet-name>addstudent.jsp</servlet-name>
    	<jsp-file>/addstudent.jsp</jsp-file>
	</servlet>
	
	<servlet-mapping>
	    <servlet-name>addstudent.jsp</servlet-name>
	    <url-pattern>/addstudent</url-pattern>
	</servlet-mapping>
	
	<servlet>
    	<servlet-name>viewstudent.jsp</servlet-name>
    <jsp-file>/viewstudent.jsp</jsp-file>
	</servlet>
	
	<servlet-mapping>
	    <servlet-name>viewstudent.jsp</servlet-name>
	    <url-pattern>/viewstudent</url-pattern>
	</servlet-mapping>




</web-app>